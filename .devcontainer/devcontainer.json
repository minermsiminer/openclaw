{
  "name": "OpenClaw Codespace",
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "forwardPorts": [18789, 5173, 19001],
  "postCreateCommand": "pnpm install --frozen-lockfile && pnpm check:packagejson && pnpm ui:install && pnpm ui:build && pnpm build",
  "postStartCommand": "bash .devcontainer/start-codespace.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "remoteUser": "codespace"
}
